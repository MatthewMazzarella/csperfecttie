<!DOCTYPE html>
<html>
  <head>
    <title>C's Perfect Tie</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <nav class="navbar navbar-inverse">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand"<%= link_to "C's Perfect Tie", root_path, id:"color" %></a>
        </div>
          <ul class="nav navbar-nav">
            <li><%= link_to "Home", root_path, id:"color" %></li>
            <li><%= link_to "Store", items_path, id:"color" %></li>
            <li><%= link_to "Contact", contact_path, id:"color" %></li>
            <li><%= link_to "Login", login_path,id:"color" %></li>
          </ul>
      </div>
  </nav>
    
    <%= render 'partials/jumbotron' %>

    <div class="container">
      <!-- Example row of columns -->
    <%= yield %>
  
    <%= render 'partials/footer' %>
    </div>
  </body>
</html>


